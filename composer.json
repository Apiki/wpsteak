{
  "name": "apiki/wpsteak",
  "description": "Patterns for building an application in WordPress.",
  "type": "library",
  "license": "GPL-2.0-only",
  "require": {
    "cedaro/wp-plugin": "0.4.0",
    "php": ">=7.2",
    "psr/log": "1.1.3"
  },
  "require-dev": {
    "dealerdirect/phpcodesniffer-composer-installer": "0.7.0",
    "phpcompatibility/phpcompatibility-wp": "2.1.0",
    "phpunit/phpunit": "9.1.4",
    "squizlabs/php_codesniffer": "3.9.0",
    "wp-coding-standards/wpcs": "2.3.0",
    "slevomat/coding-standard": "6.3.9",
    "edgedesign/phpqa": "1.23.3",
    "php-stubs/wordpress-stubs": "5.4.2",
    "szepeviktor/phpstan-wordpress": "0.6.0",
    "nette/neon": "3.1.2",
    "vimeo/psalm": "3.11.6"
  },
  "autoload": {
    "psr-4": {
      "WPSteak\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "WPSteak\\Test\\": "tests/"
    }
  },
  "scripts": {
    "install-codestandards": [
      "Dealerdirect\\Composer\\Plugin\\Installers\\PHPCodeSniffer\\Plugin::run"
    ],
    "lint": [
      "phpqa",
      "psalm"
    ],
    "lint:fix": "phpcbf",
    "test": "phpunit"
  }
}
